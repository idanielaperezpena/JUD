
@model Negocio.ViewModels.Domicilio.DomicilioFormViewModel



    <div class="box-header with-border">
        <h3 class="box-title">Domicilio @ViewBag.Adicional</h3>
        
    </div>

    <form role="form">
        
        @Html.HiddenFor(m=>m.DOM_IDDomicilio)
            <div class="form-group">
                @Html.LabelFor(m => m.DOM_IDVialidad)
                @Html.CustomDropDownListFor(m => m.DOM_IDVialidad, Model.Vialidad, new { @class = "form-control form-control-sm " })
                @Html.ValidationMessageFor(m => m.DOM_IDVialidad, "")
            </div>
            <div class="form-group">
                @Html.LabelFor(m => m.DOM_NombreVialidad)
                @Html.EditorFor(m => m.DOM_NombreVialidad, new { htmlAttributes = new { @class = "form-control form-control-sm" } })
                @Html.ValidationMessageFor(m => m.DOM_NombreVialidad, "")
            </div>
            <div class="row">
                <div class="form-group col-lg-6">
                    @Html.LabelFor(m => m.DOM_NumeroExterior)
                    @Html.EditorFor(m => m.DOM_NumeroExterior, new { htmlAttributes = new { @class = "form-control form-control-sm" } })
                </div>
                <div class="form-group col-lg-6">
                    @Html.LabelFor(m => m.DOM_NumeroInterior)
                    @Html.EditorFor(m => m.DOM_NumeroInterior, new { htmlAttributes = new { @class = "form-control form-control-sm" } })
                </div>
            </div>
            <div class="row">
                <div class="form-group col-lg-6">
                    @Html.LabelFor(m => m.DOM_Manzana)
                    @Html.EditorFor(m => m.DOM_Manzana, new { htmlAttributes = new { @class = "form-control form-control-sm" } })
                </div>
                <div class="form-group col-lg-6">
                    @Html.LabelFor(m => m.DOM_Lote)
                    @Html.EditorFor(m => m.DOM_Lote, new { htmlAttributes = new { @class = "form-control form-control-sm" } })
                </div>
            </div>
            <div class="row">
                <div class="form-group col-lg-6">
                    @Html.LabelFor(m => m.DOM_Colonia)
                    @Html.EditorFor(m => m.DOM_Colonia, new { htmlAttributes = new { @class = "form-control form-control-sm" } })
                    @Html.ValidationMessageFor(m => m.DOM_Colonia, "")
                </div>
                <div class="form-group col-lg-6">
                    @Html.LabelFor(m => m.DOM_IDAlcaldia)
                    @Html.CustomDropDownListFor(m => m.DOM_IDAlcaldia, Model.Alcaldia, new { @class = "form-control form-control-sm " })
                    @Html.ValidationMessageFor(m => m.DOM_IDAlcaldia, "")
                </div>
            </div>

            <div class="row">
                <div class="form-group col-lg-6">
                    @Html.LabelFor(m => m.DOM_CodigoPostal)
                    @Html.EditorFor(m => m.DOM_CodigoPostal, new { htmlAttributes = new { @class = "form-control form-control-sm" } })
                </div>
                <div class="form-group  col-lg-6">
                    @Html.LabelFor(m => m.DOM_IDEstado)
                    @Html.CustomDropDownListFor(m => m.DOM_IDEstado, Model.Estado, new { @class = "form-control form-control-sm " })
                    @Html.ValidationMessageFor(m => m.DOM_IDEstado, "")
                </div>
            </div>
            <div class="row">
                <div class="form-group col-lg-6">
                    @Html.LabelFor(m => m.DOM_Latitud)
                    @Html.EditorFor(m => m.DOM_Latitud, new { htmlAttributes = new { @class = "form-control form-control-sm" } })
                    @Html.ValidationMessageFor(m => m.DOM_Latitud, "")
                </div>
                <div class="form-group col-lg-6">
                    @Html.LabelFor(m => m.DOM_Longitud)
                    @Html.EditorFor(m => m.DOM_Longitud, new { htmlAttributes = new { @class = "form-control form-control-sm" } })
                    @Html.ValidationMessageFor(m => m.DOM_Longitud, "")
                </div>
            </div>
          
               
                   
                
           

    

        @if (Model.Boton == true)
        {
            <div class="box-footer">
                <button type="submit" class="btn btn-primary">Guardar</button>
            </div>
        }

    </form>


